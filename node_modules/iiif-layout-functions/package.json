{
  "name": "iiif-layout-functions",
  "version": "0.0.2-alpha",
  "description": "Pure layout functions for iiif sequences that arrange many canvases in space.",
  "main": "dist/iiif-layout-functions.js",
  "scripts": {
    "serve": "webpack-dev-server",
    "test": "jasmine",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat  ./coverage/lcov.info | codecov",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename dist/iiif-layout-functions.umd.js",
    "build:umd.min": "webpack --output-filename dist/iiif-layout-functions.umd.min.js",
    "watch:test": "npm t -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sul-dlss/iiif-layout-functions.git"
  },
  "keywords": [
    "iiif",
    "layouts",
    "manifest",
    "visualisation"
  ],
  "author": "aeschylus, edsilv",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "^6.8.0",
    "babel-eslint": "^4.1.6",
    "babel-istanbul": "^0.6.0",
    "babel-istanbul-loader": "0.0.1",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "etch": "https://github.com/edsilv/etch.git#dev",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "loader-utils": "^0.2.12",
    "npm-run-all": "2.3.0",
    "nyc": "7.1.0",
    "rimraf": "^2.5.0",
    "webpack": "^1.13.0"
  },
  "bugs": {
    "url": "https://github.com/sul-dlss/iiif-layout-functions/issues"
  },
  "homepage": "https://github.com/sul-dlss/iiif-layout-functions#readme"
}
